<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>
<Pane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
      prefWidth="400.0" stylesheets="@../css/global.css" xmlns="http://javafx.com/javafx/8"
      xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <ScrollPane layoutX="-1.0" prefHeight="400.0" prefWidth="200.0">
            <content>
                <AnchorPane fx:id="projectPane" minHeight="0.0" minWidth="0.0" prefHeight="395.0" prefWidth="195.0"/>
            </content>
        </ScrollPane>
        <Button layoutX="260.0" layoutY="25.0" mnemonicParsing="false" onAction="#newProjectClick" prefWidth="80.0"
                styleClass="button-import" text="New">
            <graphic>
                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/new_project_ico.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
        <Button layoutX="260.0" layoutY="75.0" minHeight="35.0" mnemonicParsing="false" onAction="#openProjectClick"
                prefHeight="25.0" prefWidth="80.0" styleClass="button-import" text="Open">
            <graphic>
                <ImageView fitHeight="25.0" fitWidth="25.0" pickOnBounds="true" preserveRatio="true">
                    <image>
                        <Image url="@../img/open_project_ico.png"/>
                    </image>
                </ImageView>
            </graphic>
        </Button>
    </children>
</Pane>
